/* updating multiple tables with NOT NULLS  */

ALTER TABLE orders
MODIFY order_date DATE NOT NULL;

ALTER TABLE orders
MODIFY order_items VARCHAR(200) NOT NULL;

ALTER TABLE orders
MODIFY order_items_quantity INT NOT NULL;

ALTER TABLE orders
MODIFY order_amount NUMBER NOT NULL; 

ALTER TABLE orders
MODIFY payment_method VARCHAR(20) NOT NULL;

ALTER TABLE orders
MODIFY purchase_type VARCHAR(20) NOT NULL;

ALTER TABLE orders
MODIFY delivery VARCHAR(10) NOT NULL;

ALTER TABLE orders
ADD FOREIGN KEY (customer_id) REFERENCES customers (customer_id);

ALTER TABLE customers
MODIFY first_name VARCHAR(50) NOT NULL;

ALTER TABLE customers
MODIFY last_name VARCHAR (100) NOT NULL;

ALTER TABLE customers
MODIFY email VARCHAR(100) NOT NULL;

ALTER TABLE customers
MODIFY phone_number VARCHAR(25) NOT NULL; 

ALTER TABLE customers
MODIFY delivery_address VARCHAR(200) NOT NULL;

ALTER TABLE customers
MODIFY city VARCHAR (50) NOT NULL;

ALTER TABLE customers
MODIFY postal_code VARCHAR (25) NOT NULL;

ALTER TABLE customers
MODIFY country VARCHAR (50) NOT NULL; 
