/* After inserting all the data I have decided to add type of purchase
and payment method, which is shown in the next lines of the code. */

ALTER TABLE sales 
ADD purchase_type varchar(20);

UPDATE sales 
SET purchase_type =  
    CASE WHEN order_id IN (177, 92, 122, 262, 6, 211, 42, 288, 283, 202, 75, 247, 275, 83, 187, 169, 220, 258, 147, 216, 234, 171, 248, 207, 284, 60, 95, 34, 74, 65, 102, 53, 221, 165, 266, 72, 274, 71, 76, 139, 69, 46, 201, 62, 282, 100, 152, 196, 17, 280, 261, 265, 89, 279, 229, 237, 243, 20, 226, 294, 175, 21, 101, 209, 118, 1, 134, 242, 44, 16, 290, 188, 85, 2, 208, 257, 186, 246, 213, 73, 19, 39, 138, 191, 166, 132, 259, 23, 210, 99, 123, 78, 245, 98, 200, 172, 240, 22, 238, 295, 286, 129, 167, 32, 194, 124, 244, 253, 296, 119, 49, 14, 13, 47, 218, 82, 105, 239, 126, 230, 267, 146, 151, 64, 15, 263, 251, 254, 264, 271, 287, 289, 144, 58) 
    THEN 'online' 
    ELSE 'store' 
    END;

ALTER TABLE sales 
ADD payment_method varchar(20);

UPDATE sales 
SET payment_method =  
    CASE WHEN order_id IN (177, 92, 122, 262, 258, 71, 76, 139, 69, 46, 201, 62, 282, 100, 152, 196, 17, 280, 261, 265, 89, 279, 229, 237, 243, 20,  
    226, 294, 175, 21, 101, 209, 118, 1, 134, 242, 44, 16, 290, 188, 85, 2, 208, 257, 186, 246, 213, 73, 19, 39, 138, 191, 166, 132, 259, 23, 210, 99, 123, 78, 245, 98, 200, 172, 240, 22, 238, 295,  
    286, 129, 167, 218, 82, 105, 239, 126, 230, 267, 146, 151, 64, 15, 263, 251, 254, 264, 271, 287, 289, 144, 58) 
    THEN 'money transfer' 
    WHEN order_id IN(6, 211, 42, 288, 283, 202, 75, 247, 275, 83, 187, 169, 220, 32, 194, 124, 244, 253, 296, 119, 49, 14, 13, 47) THEN 'card' 
    WHEN order_id IN(147, 216, 234, 171, 248, 207, 284, 60, 95, 34, 74, 65, 102, 53, 221, 165, 266, 72, 274) THEN 'gift card' 
    ELSE 'cash' 
    END;

UPDATE sales 
SET payment_method = 'card' 
    WHERE order_id IN (241,269,225,249,276,268,272,57,127,293,281,231,281,255,205,227,236,270,205,214,212,77,31,37,12,7,55,81,67,35,79,66,7,81,10,30);

/*	ORDERS TABLE		*/

CREATE TABLE orders (
    order_id                INT,
    order_date              DATE,
    order_items             VARCHAR(200),
    order_items_quantity    INT,
    order_amount            NUMBER,
    payment_method          VARCHAR(20),
    purchase_type           VARCHAR(20),
    delivery                VARCHAR(10),
  );

/* I've put payment_method and purchase_type to the orders table because 
I decided it makes more sense there */

/* actually moving the data for order_id, payment_method and purchase_type */
INSERT INTO orders(order_id, payment_method, purchase_type)
SELECT DISTINCT(sales.order_id), sales.payment_method, sales.purchase_type FROM sales;

/* update column order_item, order_items_quantity and order_amount in the orders table  */
MERGE INTO orders
    USING (
        SELECT s.order_id,
           LISTAGG(p.product_id, ', ') WITHIN GROUP (ORDER BY p.product_id) AS order_items,
           COUNT(s.product_id * s.quantity) AS order_items_quantity,
           SUM(s.quantity * p.price) AS order_amount
        FROM sales s
        JOIN products p ON s.product_id = p.product_id
        GROUP BY s.order_id) merged_data ON (orders.order_id = merged_data.order_id)
WHEN MATCHED THEN
    UPDATE SET orders.order_items = merged_data.order_items,
               orders.order_items_quantity = merged_data.order_items_quantity,
               orders.order_amount = merged_data.order_amount;

/* updating remaining columns: delivery, order_date */
UPDATE orders
SET delivery = 
CASE WHEN purchase_type = 'store' THEN 'no' ELSE 'yes' END;

UPDATE orders
SET order_date = 
    CASE order_id
        WHEN 1 THEN '01-MAR-21'
        WHEN 2 THEN '01-MAR-21'
