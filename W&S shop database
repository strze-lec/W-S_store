/*deleting the payment_method and purchase_type column from the sales table */

ALTER TABLE sales
DROP column payment_method; 

ALTER TABLE sales
DROP COLUMN purchase_type;

/* putting constraints in place in the tables */

ALTER TABLE orders
ADD PRIMARY KEY (order_id);

ALTER TABLE sales
ADD FOREIGN KEY (order_id) REFERENCES orders (order_id);


/* creating a sequence to make order_id numbers */

CREATE SEQUENCE seq_order_id   
MINVALUE 300
START WITH 300  
INCREMENT BY 1   
CACHE 10  
NOCYCLE;

/*	CREATING CUSTOMERS TABLE	*/


CREATE TABLE customers (
    customer_id         INT,
    first_name          VARCHAR(50),
    last_name           VARCHAR (100),
    email               VARCHAR(100),
    phone_number        VARCHAR(25),
    delivery_address    VARCHAR(200),
    city                VARCHAR (50),
    postal_code         VARCHAR (25),
    country             VARCHAR (50),

    PRIMARY KEY (customer_id)
);

/*  creating a sentence for new customer_id  */
CREATE SEQUENCE seq_customer_id
MINVALUE 1
START WITH 1
INCREMENT BY 1   
CACHE 10  
NOCYCLE;

/*  adding the customer_id column for table orders  */
ALTER TABLE orders
ADD customer_id  INT;

