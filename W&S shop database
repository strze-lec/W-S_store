/*	TABLE PRODUCTS	*/
CREATE TABLE products    
	(product_id	INT,    
	category 	VARCHAR(50) NOT NULL,    
	gender		VARCHAR(20) NOT NULL,        
	age_group 	VARCHAR(20) NOT NULL,    
    	price 		NUMBER NOT NULL, 

PRIMARY KEY(product_id)
    );

/* In Oracle autoincrement method works by using a sequence,
so I've created the sequence to generate product_id number */

CREATE SEQUENCE seq_product_id  
MINVALUE 1  
START WITH 1  
INCREMENT BY 1  
CACHE 10 
NOCYCLE;

